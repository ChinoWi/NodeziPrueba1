.sectionSearch
  margin 0
  padding 0
  background #62DD54
  .content
    padding 10px
    color white
    &-icon
      i
        padding 10px
      .span
        a
          padding 10px
          color #9DE895
    &-search
      padding 15px 20px 5px 20px
      input
        height 33px
        border 0
        box-shadow none !important
        background transparent
        border-bottom 1px solid white
        color white
        font-size 20px
        padding-top 10px
      ::-moz-placeholder
        color whitesmoke
      ::-webkit-input-placeholder
        color whitesmoke

.sectionSearchContent
  background white
  .resultadoSearch
    background #E9EBEE
    padding 7px
    border-bottom 0.5px solid #ADB2BB
    .text-resultado
      margin-left 10px
      color #ADB2BB
      font-size 11px
  .list-resultado
    cursor pointer
    padding 5px
    .list
      padding-left 10px
      padding-right 10px
      border-bottom 0.5px solid #ADB2BB

